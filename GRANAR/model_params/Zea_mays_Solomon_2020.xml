<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="3" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.011" n_layers="1" layer_diameter = "0.488" order="1"/>
	<pericycle cell_diameter="0.016" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.0115" n_layers="1"  order="3"/>
	<cortex cell_diameter="0.034" n_layers="5"  order="4"/>
	<exodermis cell_diameter="0.027" n_layers="3" order="5"/>	
	<epidermis cell_diameter="0.011" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.081" n_files="7" order="1.5" ratio ="2" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0.14" n_files="12"/>

</granar>