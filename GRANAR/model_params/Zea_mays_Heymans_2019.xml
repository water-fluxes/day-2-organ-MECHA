<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="1" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.011" n_layers="1" layer_diameter = "0.21" order="1"/>
	<pericycle cell_diameter="0.011" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.03" n_layers="1"  order="3"/>
	<cortex_inner cell_diameter="0.02" n_layers="1"  order="4"/>
	<cortex cell_diameter="0.03" n_layers="3"  order="4"/>
	<cortex_outer cell_diameter="0.02" n_layers="3"  order="5"/>
	<exodermis cell_diameter="0.03" n_layers="1" order="6"/>	
	<epidermis cell_diameter="0.02" n_layers="1" order="7"/>


	<!-- Vessels-->
	<xylem max_size="0.04" n_files="5" order="1.5" ratio ="1.86" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0" n_files="10"/>
	<inter_cellular_space ratio="0" size="0"/>
	

</granar>