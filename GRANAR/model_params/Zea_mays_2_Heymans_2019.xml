<?xml version="1.0" encoding="utf-8"?>

<granar>
	
	<!-- General parameters -->
	<planttype param="1" /> <!-- 1 = monocot / 2 = dicot -->
	<randomness param="1" />
	
	<!-- Cell layers -->
	<stele cell_diameter="0.009" n_layers="1" layer_diameter = "0.140" order="1"/>
	<pericycle cell_diameter="0.007" n_layers="1"  order="2"/>
	<endodermis cell_diameter="0.007" n_layers="1"  order="3"/>
	<cortex_inner cell_diameter="0.015" n_layers="1"  order="4"/>
	<cortex cell_diameter="0.0154" n_layers="4"  order="4"/>
	<exodermis cell_diameter="0.013" n_layers="1" order="5"/>	
	<epidermis cell_diameter="0.006" n_layers="1" order="6"/>


	<!-- Vessels-->
	<xylem max_size="0.025" n_files="5" order="1.5" ratio ="1.21" />
	<phloem max_size="0.01" n_files="3" />
	
	<!-- Other cell types -->
	<!--aerenchyma proportion="0" n_files="10"/-->
	<aerenchyma proportion="0.0" n_files="10"/>

</granar>